LET
	d1 == {1,2,3} = {1,2,2,3,3}
	d2 == {1,2} # {1}
	d3 == 1 \in {1}
	d4 == 1 \notin {2}
	d5 == {1} \cup {2} = {1,2}
	d6 == {1,2,3} \cap {2,3,4} = {2,3}
	d7 == {1,2} \subseteq {1,2,3}
	d8 == {1,2} \ {1} = {2}
	d9 == {x \in 1..10 : x % 2 = 0} = {2*x : x \in 1..5 }
	d10 == SUBSET {1,2} = {{},{1},{2},{1,2}}
	d11 == UNION {{},{1},{2},{1,2}} = {1,2}
IN 
	d1 /\ d2 /\ d3 /\ d4 /\ d5 /\ d6 /\ d7 /\ d8 /\ d9 /\ d10 /\ d11